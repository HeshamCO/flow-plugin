{
	"name": "Flow-plugin",
	"description": "Sketch plugin for Flow design handoff â€“ publish designs with full prototype flows for developer inspection",
	"version": "2.4.1",
	"engines": {
		"sketch": ">=53.0"
	},
	"skpm": {
		"name": "Flow-plugin",
		"manifest": "src/manifest.json",
		"main": "Flow.sketchplugin",
		"assets": [
			"assets/**/*"
		],
		"sketch-assets-file": "sketch-assets/icons.sketch"
	},
	"scripts": {
		"build:webview": "vite build",
		"dev:webview": "vite build --watch",
		"build:native": "skpm-build",
		"build": "npm run build:webview && npm run build:native",
		"watch": "skpm-build --watch",
		"start": "skpm-build --watch --run",
		"postinstall": "npm run build && skpm-link",
		"log": "skpm log -f",
		"check": "svelte-check --tsconfig ./src/webview/tsconfig.json",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "eslint 'src/**/*.{ts,svelte}'"
	},
	"devDependencies": {
		"@skpm/builder": "^0.9.5",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/svelte": "^5.2.7",
		"@tsconfig/svelte": "^5.0.4",
		"eslint": "^9.19.0",
		"jsdom": "^26.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.19.9",
		"svelte-check": "^4.1.5",
		"typescript": "^5.7.3",
		"vite": "^6.1.0",
		"vite-plugin-singlefile": "^2.0.3",
		"vitest": "^3.0.5"
	},
	"author": "Hesham <heshamidhi@gmail.com>",
	"dependencies": {
		"sketch-module-web-view": "^3.5.1"
	}
}
